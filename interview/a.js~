
const objArr = [{
        name: 'wds'
      },{
        name: 'xjx'
      },{
        name: 'wrq'
      },{
        name: 'xch'
      },{
        name: 'xjx'
      }]

      const obj = {}
      const newObjArr = []
      for(let i = 0; i < objArr.length; i++){
         if(!obj[objArr[i].name]){
          newObjArr.push(objArr[i]);
          obj[objArr[i].name] = true
         }
      }

      console.log(newObjArr)
