  <ul>
    <li>name : <span></span></li>
    <li>age : <span></span></li>
    <li>number : <span></span></li>
  </ul>
<div>
<script>

var area = document.querySelector("#list");
var fir = document.getElementsByTagName("span");

var url = "/students";
var list = [];
function foo(len, data){
  for(let i = 0; i < len; i++){
    list.push(data.students[i].name);
    list.push(data.students[i].age);
    list.push(data.students[i].number);
  }
  for(let i = 0; i < list.length; i++){
    fir[i].innerText = list[i];
  }
}

fetch(url)
.then(response=>response.json())
.then(data=>{
    console.log(data);
    area.innerText = data[2].name;
    //fir[0].innerText = data[2].students[0].name;
    foo(data[2].students.length,  data[2]);

})
.catch(err=>console.log("Fetch come cross :"+err));
</script>
